{
  "version": 2,
  "framework": null,

  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },

  "routes": [
    { "src": "/build/(.*)", "dest": "/public/build/$1" },
    { "src": "/storage/(.*)", "dest": "/public/storage/$1" },
    { "src": "/assets/(.*)", "dest": "/public/storage/assets/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ],

  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie"
  }
}
